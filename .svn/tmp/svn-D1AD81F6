<template>
  <div id="app">
    <router-view/>
  </div>
</template>
<script>
import { mapActions } from "vuex";

export default {
  name: "app",
  // Life sycle hooks.
  created() {
    this.fetchsanatext();
  },

  methods: {
    ...mapActions("content", {
      fetchsanatext: "FETCH_ALL_SANATEXT"
    })
  }
};
</script>
