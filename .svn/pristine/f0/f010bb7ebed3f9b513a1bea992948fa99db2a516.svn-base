// Cotent Mutations.

import { UPDATE_SANATEXT_SATE } from "../../mutation-types";

export default {
  // update sanatext state in vuex store.(storename: SanaTexts)
  [UPDATE_SANATEXT_SATE]: (state, payload) => {
    const curentState = state;
    if (payload) {
      curentState.SanaTexts = payload;
    }
  }
};
