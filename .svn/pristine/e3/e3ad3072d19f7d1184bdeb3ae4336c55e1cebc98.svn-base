// Contetnt Getters.

import { GET_SANA_TEXT } from "../../getter-types";

export default {
  // get sana text by key (key: sanatext key).
  // return : array of product list items
  [GET_SANA_TEXT]: state =>
    (key) => {
      if (state.SanaTexts) {
        return state.SanaTexts.find(s =>
          s.Key === key);
      }
      return {};
    }
};
