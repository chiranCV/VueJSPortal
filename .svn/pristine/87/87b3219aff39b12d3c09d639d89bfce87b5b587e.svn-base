<template>
  <sana-dropdown
    v-bind:DataList="this.sortOptionsModel"
    IdField="Id"
    TextField="Text"
    DefaultText="Select Type"
    DefaultTextSanaTextKey="IOO_GlobalInventoryCheck_Type_DefaultText"
  ></sana-dropdown>
</template>

<script>
import Dropdown from "./ui/Dropdown.vue";

export default {
  name: "SortingDropDown",
  components: {
    "sana-dropdown": Dropdown
  },
  props: {
    SortOptions: {
      type: Object
    }
  },
  computed: {
    sortOptionsModel() {
      const items = [];
      /* const optionsArray =        this.SortOptions && this.SortOptions.Fields
          ? this.SortOptions.Fields
          : []; */

      let optionsArray = [];
      if (this.SortOptions && this.SortOptions.Fields) {
        optionsArray = this.SortOptions.Fields;
      }

      if (this.SortOptions && this.SortOptions.RelevanceAvailable) {
        items.push({
          Id: this.GetSanaText("IOO_Relevance", "Relevance"),
          Text: this.GetSanaText("IOO_Relevance", "Relevance")
        });
      }
      if (optionsArray.length > 0) {
        optionsArray.forEach((item) => {
          items.push({
            Id: item.Name,
            SortOrder: "asec",
            Text: `<a><span>${
              item.Title ? item.Title : item.Name
            } </span><span><img src="data:image/png;base64," class="img-fluid"></span></a>`,
            InnerHtml: ""
          });
          items.push({
            Id: item.Name,
            SortOrder: "desc",
            Text: `<a><span>${
              item.Title ? item.Title : item.Name
            } </span><span><img src="data:image/png;base64," class="img-fluid"></span></a>`,
            InnerHtml: ""
          });
        });
      }
      return items;
    }
  }
  // methods: {  }
};
</script>
