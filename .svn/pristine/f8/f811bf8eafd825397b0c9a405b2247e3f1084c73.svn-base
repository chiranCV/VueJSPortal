// button
.btn-checkout {
  width: 100%;
  margin-top: 4px;
}
.check-inv-btn {
  width: 62px;
  height: 33px;
  font-family: "Verdana", sans-serif;
  text-align: center;
  padding: 5px 6px;
  background-color: #fff;
  border: 1px solid $zf-color-cyan;
  border-radius: 0;
  margin-top: 5px;
  span {
    font-size: 9px;
    line-height: 11px;
    color: $zf-color-cyan;
  }
  &:hover {
    background-color: $zf-color-cyan;
    span {
      color: #fff;
    }
  }
}

.cart-dropdown-container {
  float: left;
  padding-right: 15px;
  padding-top: 10px;
  padding-left: 30px;

  ul {
    width: 318px;
    padding: 20px;

    li {
      .drop-down-img {
        width: 108px;
        height: 78px;
      }
    }
  }
}

.cus-info-block {
  display: table;
  float: right;
  text-align: right;
  width: 200px;
  margin-top: 11px;
  .zfbtn {
    padding-right: 15px;
  }

  .dropdown-toggle {
    float: left;
  }

  .iconWarn {
    width: 18px;
    position: relative;
    top: -2px;
    margin: 0 5px;
  }
  .zfbtn {
    position: relative;
  }
  .downArrow {
    &:before {
      content: "";
      background-image: url("./assets/arrow-blue-down.svg");
      background-repeat: no-repeat;
      width: 11px;
      height: 6px;
      position: absolute;
      right: 0;
      top: 10px;
    }
  }

  .dropdown-menu {
    width: 410px;
    padding: 20px 10px 20px 20px;

    .search-block {
      position: relative;
      margin-bottom: 25px;

      img {
        width: 18px;
        position: absolute;
        bottom: 8px;
        left: 12px;
      }

      input {
        border-width: 0px 0px 1px 0px;
        border-color: #f2f2f2;
        border-radius: 0;
        height: 39px;
        width: 100%;
        padding-left: 40px;

        &:focus {
          outline: none;
        }
      }
    }

    .application-list-block {
      padding-left: 0;
      list-style: none;
      max-height: 275px;
      overflow-y: auto;
      padding-right: 20px;

      li {
        a {
          width: 100%;
          font-family: "Verdana", sans-serif;
          font-size: 12px;
          font-weight: bold;
          line-height: 16px;
          text-align: center;
          border: 1px solid #bfbfbf;
          margin-top: 10px;
          color: $zf-color-cyan;
          padding: 10px;
          margin: 0 0 20px 0;
        }
      }
    }

    .primaryBtn {
      width: 100%;
      font-family: "Verdana", sans-serif;
      font-size: 14px;
      line-height: 16px;
      text-align: center;
      min-height: 37px;
      margin: 20px 0 0 0;
    }
  }
}

.search-container {
  width: 543px;
  margin-top: 40px;
  border: 1px solid #bfbfbf;
  input {
    max-width: 428px;
    height: 40px;
    font-family: "Verdana", sans-serif;
    border-radius: 0 !important;
    font-size: 14px;
    line-height: 16px;
    border: none;
    &:focus {
      box-shadow: none;
    }
  }
  &.full-width {
    width: 100%;
    margin-top: 15px;
    input {
      max-width: 100%;
    }
  }
  .btn-search-icon {
    margin-top: 6px;
  }
}

.list-action-container {
  margin-top: 50px;
  margin-bottom: 40px;

  ul {
    list-style: none;

    li {
      float: left;

      &:nth-child(2) {
        margin: 0px 20px;
      }

      .text-view {
        padding-top: 3px;
      }
    }
  }

  .view-list-grid-block {
    float: left;
    padding-left: 20px;
  }

  .sort-by-container {
    float: right;
    display: inline-flex;
    padding-left: 15px;

    p {
      padding-top: 3px;
      padding-right: 20px;
    }

    .dropdown {
      top: -9px;
      .btn {
        width: 250px;
        border: 1px solid #bfbfbf;
        padding: 0;
        background-color: transparent;
        text-align: left;
        border-radius: 0;
        span {
          color: $black;
          padding: 10px;
          position: relative;
          top: 6px;
          font-family: Arial;
          font-size: 14px;
          line-height: 16px;
          a {
            padding-top: 5px;
          }
        }
      }

      ul {
        padding-left: 0;
        list-style: none;
        padding: 0;
        li {
          width: 100%;
          float: left;
          margin: 0;
          position: relative;
          padding: 0;
          &:first-child {
            background-color: $zf-color-dark-white;
          }
          &:hover {
            background-color: $zf-color-dark-white;
          }

          > a {
            color: $black;
            font-family: Arial;
            font-size: 14px;
            line-height: 16px;
            width: 100%;
            padding: 10px 15px;
            span {
              color: $black;
              font-family: Arial;
              font-size: 14px;
              line-height: 16px;
            }
            img {
              padding-left: 5px;
              width: 18px;
            }
          }
        }
      }
    }

    .down-arrow {
      position: relative;
      background-color: #7f7f7f;
      width: 40px;
      height: 39px;
      float: right;
      border-radius: 0;
      &:before {
        content: "";
        background-image: url("./assets/arrow-white-down.svg");
        background-repeat: no-repeat;
        width: 11px;
        height: 6px;
        position: absolute;
        left: 16px;
        top: 18px;
      }
    }
  }
}

.search-list-container {
  border: 1px solid $black-lighter;
  margin-bottom: 20px;
  min-height: 82px;
  .check-inv-btn {
    white-space: normal;
    min-width: 65px;
    border: 1px solid #00abe7;
  }
  h6 {
    font-size: 18px;
    line-height: 23px;
    padding-top: 10px;
    max-width: 240px;
    font-family: "Verdana", sans-serif;
    white-space: nowrap;
    overflow: hidden !important;
    text-overflow: ellipsis;
    margin-bottom: 5px;
  }

  ul {
    list-style: none;
    padding-left: 0;
    width: 100%;
    margin-bottom: 0;

    li {
      float: left;
      padding-right: 21px;
    }
  }

  span {
    float: left;
  }

  .list-avatar-block {
    // background-color: #f2f2f2;
    padding: 8px 12px;
    border-right: 1px solid $black-lighter;
    height: 100%;
    width: 115px;
    img {
      width: 90px;
      height: 69px;
      max-width: 100%;
      object-fit: cover;
    }
  }

  a {
    color: $black-light;
  }
  .list-dropdown-text {
    padding-bottom: 5px;
    position: relative;

    .zfbtn {
      padding-left: 20px;
    }

    ul {
      min-width: 1vw;
      min-width: 125px;
      padding-bottom: 0;
      padding: 0;
      right: 0px;
      left: auto !important;

      li {
        width: 100%;
        padding: 5px 5px 0 5px;
        a {
          color: $black !important;
        }
      }
    }
  }

  .down-arrow {
    &:before {
      content: "";
      background-image: url("./assets/arrow-blue-down.svg");
      background-repeat: no-repeat;
      width: 11px;
      height: 6px;
      position: absolute;
      right: -15px;
      top: 5px;
    }
  }

  .v-center-block {
    padding-top: 24px;
  }

  .v-center-btn-block {
    padding-top: 20px;
    float: right;
  }

  .list-price-text {
    font-family: Arial;
    font-size: 16px;
    font-weight: bold;
    line-height: 18px;
    text-align: right;
    color: #000000;
    float: right;
  }

  .auro-increment-input {
    input {
      padding: 5px;
      text-align: center;
      max-width: 75px;
    }

    span {
      display: block;
      float: none;
      text-align: center;
      padding-top: 4px;
    }
  }

  .listThirdBlock {
    float: right;
    padding-right: 0;
  }
  .ioo-btn-default {
    .btn-inner {
      width: 100%;
    }
  }
  .check-inv-btn {
    margin-top: 0;
    height: 40px;
  }
  .qty-box-block {
   margin-top: 20px;
   input {
     height: 40px;
   }
   .quantity-box-pm-ic-block {
    top: 8px;
   }
  }
}
.btn-cart-white {
  margin-top: 2px;
}
.ioo-btn {
  span {
    &.btn-inner {
      span {
        &.btn-cart-white {
          &:before {
            width: 22px !important;
          }
        }
      }
    }
  }
}
.bottom-container {
  margin-top: 48px;

  p {
    color: $black-light;
    font-family: Arial;
  }

  .paggination-block {
    display: table;
    margin: 0px auto;

    ul {
      list-style: none;
      padding-left: 0;

      li {
        float: left;
        padding-right: 20px;
        &.disabled {
          a {
            color: $black-light !important;
          }
        }
        &.active {
          a {
            color: $black !important;
          }
        }

        a {
          font-size: 12px;
          font-weight: bold;
          line-height: 16px;

          img {
            width: 8px;
            position: relative;
            top: -1px;
          }

          &:hover {
            text-decoration: none;
          }

          &.active {
            color: $black;
          }
        }

        &.pagination-dots {
          a {
            color: $black-light;
          }
        }
      }
    }
  }
}

.show-by-container {
  float: right;
  display: inline-flex;
  padding-left: 15px;
  margin-top: -3px;

  p {
    padding-top: 3px;
    padding-right: 20px;
    color: $black;
  }

  .dropdown {
    top: -7px;

    ul {
      min-width: 80px;

      li {
        width: 100%;
        float: left;
        padding: 10px;

        a {
          color: $black !important;
        }
      }
    }

    .btn {
      width: 80px;
      border: 1px solid #bfbfbf;
      padding: 0;
      background-color: transparent;
      text-align: left;
      border-radius: 0;
      span {
        color: #000;
        position: relative;
        top: 6px;
        font-family: Arial;
        font-size: 14px;
        line-height: 16px;
        width: 38px;
        text-align: center;
        display: inline-block;
        box-sizing: border-box;
        left: -1px;
      }
    }
  }

  .down-arrow {
    position: relative;
    background-color: #7f7f7f;
    width: 40px;
    height: 39px;
    float: right;
    border-radius: 0;
    &:before {
      content: "";
      background-image: url("./assets/arrow-white-down.svg");
      background-repeat: no-repeat;
      width: 11px;
      height: 6px;
      position: absolute;
      left: 16px;
      top: 18px;
    }
  }
}

.dotted-border-block {
  width: 100%;
  margin-top: 30px;
  height: 13px;
  float: left;
  position: relative;

  span {
    position: absolute;
    top: 0;
    bottom: 0;
    height: 10px;
    left: 0;
    right: 0;
    width: auto;
    background-image: url("./assets/dot.png");
    background-repeat: repeat-x;
    background-size: 12px;
    background-position-y: center;
  }
}

.online-shop-link-txt {
  padding-top: 16px;
  a {
    span {
      span {
        width: 100%;
      }
    }
  }
  span {
    font-size: 14px;
    line-height: 16px;
    text-align: center;
    float: left;

    &.online-shop-link-txt-img {
      padding-top: 5px;
      padding-right: 10px;

      img {
        width: 5px;
      }
    }
  }
}
.modal-dialog {
  min-width: 623px;
  max-width: 900px;
}
h4,
h6 {
  width: 100%;
  float: left;
}
.modal-header {
  display: block;
  align-items: baseline;
  position: relative;
  border-bottom: none;
  padding: 30px 30px 0 30px;

  .close {
    margin: 0;
    position: absolute;
    top: 15px;
    padding: 0;
    right: 15px;

    span {
      font-size: 30px;
      color: $zf-color-blue;
    }
  }
}
.modal-body {
  padding: 30px 10px 0 30px;
  height: 350px;
  overflow: auto;
  th {
    padding: 15px 15px 15px 0;
    text-align: left;
  }
  td {
    padding: 15px 15px 15px 0;
    text-align: left;
    &:last-child {
      padding: 15px;
    }
    &:nth-last-child(2) {
      padding: 15px;
    }
  }
}
.ioo-table {
  display: table;
  width: 100%;
  .ioo-table-body {
    display: table-row-group;
    .ioo-table-cell {
      color: #000000;
      border-top: 1px solid #ddd;
    }
  }
  .ioo-table-row {
    display: table-row;
    .ioo-table-head {
      color: $black-light;
      font-weight: bold !important;
    }
    .ioo-table-cell,
    .ioo-table-head {
      display: table-cell;
      padding: 3px 10px;
      font-family: "Verdana", sans-serif;
      font-size: 11px;
      line-height: 14px;
      padding: 15px 10px 15px 0 !important;
    }
  }
}
.uploadStatusPP {
  .modal-header {
    float: left;
    width: 100%;
    padding: 30px 15px 20px 15px;
  }
  .modal-body {
    float: left;
  }
  .btnBlock {
    width: 100%;
    margin: 20px 0 40px 0;
    float: left;
    .secondryBtnList {
      width: 48%;
      margin-right: 2%;
    }
    .secondryCartBtnI {
      width: 48%;
      margin-left: 2%;
      text-align: center;
      &::after {
        right: 16px;
      }
    }
  }
  .quantityBoxBlock {
    span {
      width: 70px;
      display: block;
      text-align: center;
      padding-top: 3px;
    }
    input {
      height: 37px;
      width: 70px;
    }
  }
  .clickableLink {
    padding-top: 10px;
  }
}

.vdp-datepicker__calendar header {
  height: auto;
}
.list-action-container {
  &.list-active {
    .list-non-active-img,
    .grid-active-img {
      display: none;
    }
    .list-active-img {
      display: block;
    }
  }
  &.grid-active {
    .grid-non-active-img,
    .list-active-img {
      display: none;
    }
    .grid-active-img {
      display: block;
    }
  }
}
.search-result-text {
  p {
    margin-top: 3px;
  }
}
.search-error-msg {
  padding-top: 40px;
}
@media screen and (min-width: 1199px) {
  .ioo-dpl-55 {
    padding-left: 44px;
  }
}
@media screen and (max-width: 1199px) {
  .iooContainer {
    .ioo-btn {
      min-width: 130px;
    }
    .list-action-container {
      .sort-by-container {
        .dropdown {
          .btn {
            width: 160px;
          }
        }
      }
    }
  }
  .product-grid-container {
    .btn-block {
      a {
        float: none !important;
      }
    }
  }
}
@media screen and (max-width: 1366px) {
  .search-list-container {
    .qty-box-block {
      input {
        width: 65px;
      }
    }
  }
  .pr-4 {
    padding-right: 1rem !important;

  }
}
.row.ioo-no-gutters {
  margin-right: 0;
  margin-left: 0;
}
.stext-li-container {
  max-width: 50px;
}
.sprice-li-container {
  width: 100px;
  word-break: break-all;
}
