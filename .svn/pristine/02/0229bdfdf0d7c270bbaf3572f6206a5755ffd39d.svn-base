.bottom-container {
  margin-top: 50px;

  p {
    color: $black-light;
    font-family: Arial;
  }

  .paggination-block {
    display: table;
    margin: 0px auto;

    ul {
      list-style: none;
      padding-left: 0;

      li {
        float: left;
        padding-right: 20px;

        a {
          font-size: 12px;
          font-weight: bold;
          line-height: 16px;

          img {
            width: 8px;
            position: relative;
            top: -1px;
          }

          &:hover {
            text-decoration: none;
          }

          &.active {
            color: $black;
          }
        }

        &.pagination-dots {
          a {
            color: $black-light;
          }
        }
      }
    }
  }
}

