.dotted-border-block {
  width: 100%;
  margin-top: 40px;
  height: 13px;
  float: left;
  position: relative;

  span {
    position: absolute;
    top: 0;
    bottom: 0;
    height: auto;
    left: 0;
    right: 0;
    width: auto;
    background-image: url("./assets/dot.png");
    background-repeat: repeat-x;
    background-size: 12px;
  }
}

.online-shop-link-txt {
