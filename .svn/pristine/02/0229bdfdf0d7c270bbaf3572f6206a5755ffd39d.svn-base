}
  .modal-dialog {
    max-width: 623px;
  }
  h4,
  h6 {
    width: 100%;
    float: left;
  }
  .modal-header {
    display: block;
    align-items: baseline;
    position: relative;
    border-bottom: none;
    padding: 30px 30px 0 30px;

    .close {
      margin: 0;
      position: absolute;
      top: 15px;
      padding: 0;
      right: 15px;

      span {
        font-size: 30px;
        color: $zf-color-blue;
      }
    }
    
  }
  .modal-body {
    padding: 30px 10px 0 30px;
    height: 350px;
    overflow: auto;
    th {
      padding: 15px 15px 15px 0;
      text-align: left;
    }
    td {
      padding: 15px 15px 15px 0;
      text-align: left;
      &:last-child {
        padding: 15px;
      }
      &:nth-last-child(2) {
        padding: 15px;
      }
    }
  }
  .ioo-table {
  	display: table;
    width: 100%;
    .ioo-table-body {
      display: table-row-group;
      .ioo-table-cell {
        color: #000000;
        border-top: 1px solid #ddd;
      }
    }
    .ioo-table-row {
      display: table-row;
      .ioo-table-head {
        color: $black-light;
        font-weight: bold !important;
      }
      .ioo-table-cell, .ioo-table-head {
        display: table-cell;
        padding: 3px 10px;
        font-family: "Verdana", sans-serif;
        font-size: 11px;
        line-height: 14px;
        padding: 15px 10px 15px 0 !important;
    }
  }
}
  .uploadStatusPP {
    .modal-header {
      float: left;
      width: 100%;
      padding: 30px 15px 20px 15px;
    }
    .modal-body {
      float: left;
    }
    .btnBlock {
      width: 100%;
      margin: 20px 0 40px 0;
      float: left;
      .secondryBtnList {
        width: 48%;
        margin-right: 2%;
      }
      .secondryCartBtnI {
        width: 48%;
        margin-left: 2%;
        text-align: center;
        &::after {
          right: 16px;
        }
      }
    }
    .quantityBoxBlock {
      span {
        width: 70px;
        display: block;
        text-align: center;
        padding-top: 3px;
      }
      input {
        height: 37px;
        width: 70px;
      }
    }
    .clickableLink {
      padding-top: 10px;
    }
  }

.vdp-datepicker__calendar header {
  height: auto;
}
@media screen and (min-width: 1199px) {
  .ioo-dpl-55 {
    padding-left: 44px;
  }
}
@media screen and (max-width: 1199px) {

  .iooContainer {
     .ioo-btn {
      min-width: 130px;
     }
    .list-action-container {
      .sort-by-container {
        .dropdown {
          .btn {
            width: 160px;
          }
        }
      }
    }
  }
}
.row.ioo-no-gutters {
  margin-right: 0;
  margin-left: 0;
}
.stext-li-container {
  max-width: 50px;
}
.sprice-li-container {
  width: 100px;
