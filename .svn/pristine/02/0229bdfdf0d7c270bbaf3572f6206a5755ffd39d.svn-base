.cart-dropdown-container {
  float: left;
  padding-right: 15px;
  padding-top: 10px;
  padding-left: 30px;

  ul {
    width: 318px;
    padding: 20px;

    li {


      .drop-down-img {
        width: 108px;
        height: 78px;

      }
    }
  }
}

.cus-info-block {
  display: table;
  float: right;
text-align: right;
width: 200px;
  .zfbtn {
    padding-right: 15px;
  }

  .dropdown-toggle {
    float: left;
  }

  .iconWarn {
    width: 18px;
    position: relative;
    top: -2px;
    margin: 0 5px;
  }
.zfbtn {
  position: relative;
}
  .downArrow {
    &:before {
      content: "";
      background-image: url("./assets/arrow-blue-down.svg");
      background-repeat: no-repeat;
      width: 11px;
      height: 6px;
      position: absolute;
      right: 0;
      top: 10px;
    }
  }

  .dropdown-menu {
    width: 410px;
    padding: 20px 10px 20px 20px;

    .search-block {
      position: relative;
      margin-bottom: 25px;

      img {
        width: 18px;
        position: absolute;
        bottom: 8px;
        left: 12px;
      }

      input {
        border-width: 0px 0px 1px 0px;
        border-color: #f2f2f2;
        border-radius: 0;
        height: 39px;
        width: 100%;
        padding-left: 40px;

        &:focus {
          outline: none;
        }
      }
    }

    .application-list-block {
      padding-left: 0;
      list-style: none;
      max-height: 275px;
      overflow-y: auto;
      padding-right: 20px;

      li {
        a {
          width: 100%;
          font-family: "Verdana", sans-serif;
          font-size: 12px;
          font-weight: bold;
          line-height: 16px;
          text-align: center;
          border: 1px solid #bfbfbf;
          margin-top: 10px;
          color: $zf-color-cyan;
          padding: 10px;
          margin: 0 0 20px 0;
        }
      }
    }

    .primaryBtn {
      width: 100%;
      font-family: "Verdana", sans-serif;
      font-size: 14px;
      line-height: 16px;
      text-align: center;
      min-height: 37px;
      margin: 20px 0 0 0;
    }
  }
}

.search-container {
  width: 543px;
  margin-top: 40px;
  border: 1px solid #bfbfbf;
  input {
    max-width: 428px;
    height: 40px;
    font-family: "Verdana", sans-serif;
    border-radius: 0 !important;
    font-size: 14px;
    line-height: 16px;
    border: none;
    &:focus {
      box-shadow: none;
    }
  }
}

.list-action-container {
  margin-top: 50px;
  margin-bottom: 40px;

  ul {
    list-style: none;

    li {
      float: left;

      &:nth-child(2) {
        margin: 0px 20px;
      }

      .text-view {
        padding-top: 3px;
      }
    }
  }

  .view-list-grid-block {
    float: left;
    padding-left: 20px;
  }

  .sort-by-container {
    float: right;
    display: inline-flex;
    padding-left: 15px;

    p {
      padding-top: 3px;
      padding-right: 20px;
    }

    .dropdown {
      top: -7px;
      .btn {
        width: 250px;
        border: 1px solid #bfbfbf;
        padding: 0;
        background-color: transparent;
        text-align: left;

        span {
          color: $black;
          padding: 10px;
          position: relative;
          top: 6px;
          font-family: Arial;
          font-size: 14px;
          line-height: 16px;
          a {
            padding-top: 5px;
          }
        }
      }

      ul {
        padding-left: 0;
        list-style: none;
        padding: 0;
        li {
          width: 100%;
          float: left;
          margin: 0;
          position: relative;
          padding: 0;
          &:first-child {
            background-color: $zf-color-dark-white;
          }

          > a {
            color: $black;
            font-family: Arial;
            font-size: 14px;
            line-height: 16px;
            width: 100%;
            padding: 10px 15px;
           span {
            color: $black;
            font-family: Arial;
            font-size: 14px;
            line-height: 16px;
           }
            img {
              padding-left: 5px;
              width: 18px;
            }
          }
        }
      }
    }

    .down-arrow {
      position: relative;
      background-color: #7f7f7f;
      width: 40px;
      height: 39px;
      float: right;
      border-radius: 0 3px 3px 0;
      &:before {
        content: "";
        background-image: url("./assets/arrow-white-down.svg");
        background-repeat: no-repeat;
        width: 11px;
        height: 6px;
        position: absolute;
        left: 16px;
        top: 18px;
      }
    }
  }
}

.search-list-container {
  border: 1px solid $black-lighter;
  margin-bottom: 20px;

  h6 {
    font-size: 18px;
