}
.check-inv-btn {
  width: 62px;
  height: 33px;
  font-family: "Verdana", sans-serif;
  text-align: center;
  padding: 5px 10px;
  background-color: #fff;
  border: 1px solid $zf-color-cyan;
  border-radius: 0;
  margin-top: 5px;
span{
  font-size: 9px;
  line-height: 11px;
  color: $zf-color-cyan;
}
  &:hover {
    background-color: $zf-color-cyan;
   span {
    color: #fff;
   }
