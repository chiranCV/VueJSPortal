  }

  .list-price-text {
    font-family: Arial;
    font-size: 16px;
    font-weight: bold;
    line-height: 18px;
    text-align: right;
    color: #000000;
    float: right;
  }

  .auro-increment-input {
    input {
      padding: 5px;
      text-align: center;
