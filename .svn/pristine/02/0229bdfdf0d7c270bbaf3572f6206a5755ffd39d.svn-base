    }
  }

  a {
    color: $black-light;
  }

  .list-dropdown-text {
    padding-bottom: 5px;
    position: relative;

    .zfbtn {
      padding-left: 20px;
    }

    ul {
      width: 1vw;
      min-width: 65px;
      padding-bottom: 0;
