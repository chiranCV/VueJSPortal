<template>
  <div class="app-wrapper iooContainer">
    <div class="container">
      <div>
        <router-view/>
      </div>
    </div>
  </div>
</template>
<script>
import { mapActions } from "vuex";

import {
  FETCH_ALL_SANATEXT,
  FETCH_CURRENT_BASKET,
  FETCH_SITE_SETTINGS,
  FETCH_ALL_CUSTOMER_ACCOUNT
} from "./store/action-types";

export default {
  name: "app",
  // Life sycle hooks.
  created() {
    this.fetchsitesettings();
    this.fetchsanatext();
    this.fetchCurentBaket();
    this.fetchAllCurentAccount();
  },

  methods: {
    ...mapActions("content", {
      fetchsanatext: FETCH_ALL_SANATEXT,
      fetchsitesettings: FETCH_SITE_SETTINGS
    }),
    ...mapActions("basket", {
      fetchCurentBaket: FETCH_CURRENT_BASKET
    }),
    ...mapActions("customer", {
      fetchAllCurentAccount: FETCH_ALL_CUSTOMER_ACCOUNT
    })
  }
};
</script>
