<template>
  <div>
    search result
    <sana-qtybox v-if="(!IsDataLaoding)"></sana-qtybox>
  </div>
</template>
<script>
import { mapActions } from "vuex";

import { FETCH_SEARCH_RESULT } from "../../store/action-types";

const QtyBox = () => import(/* webpackChunkName:"qtybox" */ "../shared/ui/QuantityBox.vue");

export default {
  name: "Search",

  // components.
  components: {
    "sana-qtybox": QtyBox
  },

  // local state.
  data() {
    return {
      ListItems: {},
      IsDataLaoding: false,
      searchOptions: { query: "1", pagesize: 10 }
    };
  },

  // props for the compoment.
  props: {},

  // Life sycle hooks.
  created() {
    this.IsDataLaoding = true;
    this.fetchSearchResult(this.searchOptions).then(
      (this.IsDataLaoding = false)
    );
  },

  mounted() {},

  // vue defult propetes for data manupilation.
  watch: {},

  filters: {},

  computed: {},

  methods: {
    ...mapActions("products", {
      fetchSearchResult: FETCH_SEARCH_RESULT
    })
  }
};
</script>
